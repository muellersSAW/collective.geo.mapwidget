function() { let wkt = new OpenLayers.Format.WKT({ internalProjection: widget.map.projection, externalProjection: widget.map.displayProjection });let layer = new OpenLayers.Layer.Vector('coordinates');let features = wkt.read('%s') || [];console.log('shapedisplay');wkt.destroy();if(features.constructor != Array) {features = [features];} layer.addFeatures(features);$(window).bind('maploadend', function (evt, widget) { var map = widget.map; map.zoomToExtent(layer.getDataExtent()); map.zoomTo(map.getZoom() - 4); }); return layer; }
